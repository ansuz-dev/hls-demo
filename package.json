{
  "name": "api-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "init-secrets": "node scripts/init_secrets.js",
    "start": "node ./bin/www.js",
    "migrate": "NODE_ENV=development npx sequelize db:migrate",
    "migrate-test": "NODE_ENV=test npx sequelize db:migrate",
    "migrate-beta": "NODE_ENV=beta npx sequelize db:migrate",
    "migrate-prod": "NODE_ENV=production npx sequelize db:migrate",
    "seed": "NODE_ENV=development npx sequelize db:seed:all",
    "seed-beta": "NODE_ENV=beta npx sequelize db:seed:all",
    "seed-beta-undo": "NODE_ENV=beta npx sequelize db:seed:undo:all",
    "seed-prod": "NODE_ENV=production npx sequelize db:seed:all",
    "test": "mocha --recursive --exit express/test/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ansuzdev/logger": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.2",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@keeex/eslint-config": "^1.5.7",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-promise": "^5.1.0",
    "faker": "^5.5.3",
    "grunt": "^1.4.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-eslint": "^23.0.0",
    "mjml": "^4.10.3",
    "mocha": "^9.1.2",
    "mochawesome": "^6.3.1"
  }
